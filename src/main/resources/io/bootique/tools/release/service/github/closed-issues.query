query($owner:String!, $name:String!, $totalCount: Int!) {
  repository(owner:$owner,name:$name) {
    name
      issues(last: $totalCount, states: CLOSED) {
        totalCount
        nodes{
          id
          url
          number
          title
          milestone {
            id
            url
            title
            number
          }
        }
      }
  }
}